	<script type="text/javascript">
	
var topic = 'clemsos';

$(window).bind('load',function() {
  $(document).liveTwitter(topic ,{
    rpp: 10,
    limit:1,
    mode:'user_timeline',
    retweets: 1,
    entities:1,
    filter: function(tweet){
      console.log(tweet);
    }
  })
});
	</script>
